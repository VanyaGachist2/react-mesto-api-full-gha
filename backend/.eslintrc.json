{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-undef": "off",
      "no-extra-semi": "off",
      "no-trailing-spaces": "off",
      "import/newline-after-import": "off",
      "import/order": "off",
      "import/extensions": "off",
      "semi": "off",
      "indent": "off",
      "new-cap": "off",
      "arrow-body-style": "off",
      "consistent-return": "off"
    }
}
